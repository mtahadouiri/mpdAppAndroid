<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference android:title="@string/preference_launch_faq"
        android:summary="@string/preference_launch_faq_text">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/url_faq"/>
    </Preference>
    <Preference android:title="@string/preference_launch_about"
        android:summary="@string/preference_launch_about_text">
        <intent
            android:action="android.intent.action.VIEW"
            android:targetClass="com.mtdev.musicbox.application.activities.AboutActivity"
            android:targetPackage="com.mtdev.musicbox" />
    </Preference>
</PreferenceScreen>